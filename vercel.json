{
  "version": 2,
  "name": "marc-anime",
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/health",
      "dest": "/index.js"
    },
    {
      "src": "/watch",
      "dest": "/Public/watch.html"
    },
    {
      "src": "/",
      "dest": "/Public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/Public/$1"
    }
  ]
}
